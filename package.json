{
  "name": "node-express-multitenant",
  "version": "1.0.5",
  "description": "Shared database strategy multi tenancy package",
  "type": "module",
  "author": "Shaul Zuarets",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "test": "jest",
    "watch": "npm run build:dev -- --watch",
    "build:dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "createdb": "prisma migrate dev"
  },
  "main": "dist/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shauliz/node-express-multitenant.git"
  },
  "bugs": {
    "url": "https://github.com/shauliz/node-express-multitenant/issues"
  },
  "homepage": "https://github.com/shauliz/node-express-multitenant#readme",
  "prisma": {
    "schema": "./src/app/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~16.0.0",
    "@commitlint/config-conventional": "~16.0.0",
    "@types/jest": "~27.4.0",
    "@types/node": "~16.11.21",
    "@typescript-eslint/eslint-plugin": "~5.10.1",
    "@typescript-eslint/parser": "~5.10.1",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-config-standard": "~16.0.3",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-jest": "~25.7.0",
    "eslint-plugin-json": "~3.1.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-promise": "~5.2.0",
    "husky": "~7.0.0",
    "jest": "~27.4.7",
    "jest-mock-extended": "~2.0.4",
    "prettier": "~2.5.1",
    "prisma": "~3.8.1",
    "ts-jest": "~27.1.3",
    "ts-loader": "~9.2.6",
    "tsconfig-paths-webpack-plugin": "~3.5.2",
    "typescript": "~4.5.4",
    "webpack": "~5.67.0",
    "webpack-cli": "~4.9.1",
    "webpack-merge": "~5.8.0"
  },
  "peerDependencies": {
    "@prisma/client": "^3.0.0",
    "prisma": "^3.0.0"
  },
  "engines": {
    "node": ">= 16.4"
  }
}
